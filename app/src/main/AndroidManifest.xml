<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="t.s.com">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".desgin.DesignActivity" />
        <activity android:name=".desgin.DrawerLayoutActivity" />
        <activity android:name=".desgin.ScrollviewXuanFuActivity"></activity>


        <service
            android:name="t.s.com.appservice.MyService">
            <intent-filter>
                <action android:name="t.s.com.appservice" />
            </intent-filter>
        </service>

    </application>

</manifest>